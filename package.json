{
  "name": "cycle-webrtc",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prebuild": "node bin/move-assets.js",
    "build": "npm run build:js && npm run build:sass",
    "build:js": "browserify src/js/index.js -o dist/js/app.js -t [ babelify --presets [ es2015 ] ]",
    "build:sass": "node-sass --include-path=$(node bin/sass-paths.js) src/sass/style.sass dist/css/style.css",
    "watch": "npm run watch:js & npm run watch:sass & npm run livereload",
    "watch:js": "nodemon --watch 'src/js' --exec 'npm run build:js'",
    "watch:sass": "npm run build:sass -- --watch src/sass/**/*",
    "livereload": "livereload dist",
    "start": "npm run build && serve & npm run watch"
  },
  "author": "Alex Milanov <alex@i4web.biz>",
  "license": "MIT",
  "dependencies": {
    "@cycle/core": "^6.0.0",
    "@cycle/dom": "^9.0.2",
    "@cycle/http": "^8.0.0",
    "bootstrap": "^3.3.6",
    "bootstrap-sass": "^3.3.6",
    "font-awesome": "^4.6.3",
    "rx": "^4.0.7",
    "webrtc-adapter-test": "^0.2.9"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "eslint": "^3.0.1",
    "eslint-config-google": "^0.6.0",
    "fs-extra": "^0.30.0",
    "livereload": "^0.4.1",
    "node-sass": "^3.4.2",
    "node-serve": "0.0.3",
    "nodemon": "^1.8.1"
  }
}
